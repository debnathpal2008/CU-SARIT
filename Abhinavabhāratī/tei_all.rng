      <group>
        <zeroOrMore>
          <ref name="model.headLike"/>
        </zeroOrMore>
        <oneOrMore>
          <choice>
            <ref name="model.placeLike"/>
            <ref name="listPlace"/>
          </choice>
        </oneOrMore>
        <zeroOrMore>
          <choice>
            <ref name="relationGrp"/>
            <ref name="relation"/>
          </choice>
        </zeroOrMore>
      </group>
      <ref name="att.global.attributes"/>
      <ref name="att.typed.attributes"/>
      <ref name="att.declarable.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="location">
    <element name="ns1:location">
      <a:documentation>defines the location of a place as a set of geographical coordinates, in terms of a other named geo-political entities, or as an
        address.</a:documentation>
