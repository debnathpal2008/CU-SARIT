              <ref name="model.divGenLike"/>
            </choice>
          </zeroOrMore>
        </optional>
        <choice>
          <oneOrMore>
            <ref name="model.divLike"/>
            <zeroOrMore>
              <choice>
                <ref name="model.global"/>
                <ref name="model.divGenLike"/>
              </choice>
            </zeroOrMore>
          </oneOrMore>
          <oneOrMore>
            <ref name="model.div1Like"/>
            <zeroOrMore>
              <choice>
                <ref name="model.global"/>
                <ref name="model.divGenLike"/>
              </choice>
            </zeroOrMore>
          </oneOrMore>
          <group>
            <oneOrMore>
              <ref name="model.common"/>
              <zeroOrMore>
                <ref name="model.global"/>
              </zeroOrMore>
            </oneOrMore>
            <optional>
              <choice>
                <oneOrMore>
                  <ref name="model.divLike"/>
                  <zeroOrMore>
                    <choice>
                      <ref name="model.global"/>
                      <ref name="model.divGenLike"/>
