            <optional>
              <ref name="lem"/>
              <optional>
                <ref name="wit"/>
              </optional>
            </optional>
            <group>
              <ref name="model.rdgLike"/>
              <optional>
                <ref name="wit"/>
              </optional>
            </group>
          </zeroOrMore>
        </choice>
