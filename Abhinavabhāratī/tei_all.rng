        <choice>
          <ref name="model.labelLike"/>
          <ref name="model.placeNamePart"/>
          <ref name="model.offsetLike"/>
          <ref name="model.measureLike"/>
          <ref name="model.addressLike"/>
          <ref name="model.noteLike"/>
          <ref name="model.biblLike"/>
