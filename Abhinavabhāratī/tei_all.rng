  <define name="u">
    <element name="ns1:u">
      <a:documentation>(utterance) a stretch of speech usually preceded and followed by
silence or by a change of speaker.</a:documentation>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="model.global"/>
